{
  "name": "spotilities",
  "version": "0.7.7",
  "license": "MIT",
  "private": false,
  "browserslist": [
    "> 0.25%, not dead",
    "not op_mini all",
    "not ie > 0"
  ],
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "dev:static": "yarn build && yarn export && yarn start:static",
    "build": "next build",
    "export": "next export",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:static": "http-server ./out",
    "prod": "yarn start",
    "lint": "echo \" \n\nlinting all JS and TS files.\n\n\" && yarn lint:ts; echo \"\n\nLinting styled components.\n\n\" && yarn lint:css",
    "lint:fix": "echo \" \n\nlinting all JS and TS files.\n\n\" && yarn lint:ts:fix ; echo \"\n\nLinting styled components.\n\n\" && yarn lint:css",
    "lint:ts": "eslint ./**/*.{js,jsx,tsx,ts,css}",
    "lint:ts:fix": "eslint ./**/*.{js,jsx,tsx,ts,css} --fix",
    "lint:css": "stylelint ./**/*.{js,jsx,tsx,ts}",
    "tsc": "tsc .",
    "test": "jest",
    "test:init": "jest --init",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.55",
    "@next/bundle-analyzer": "^9.3.5",
    "@reduxjs/toolkit": "^1.3.4",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "konva": "^7.2.0",
    "lodash.chunk": "^4.2.0",
    "lodash.shuffle": "^4.2.0",
    "lodash.trim": "^4.5.1",
    "lodash.uniq": "^4.5.0",
    "next": "^11.1.0",
    "next-pwa": "^2.4.1",
    "react": "16.13.1",
    "react-color": "^2.19.3",
    "react-color-extractor": "^1.1.2",
    "react-dom": "16.13.1",
    "react-ga": "^2.7.0",
    "react-hook-form": "^6.11.4",
    "react-konva": "16.13.0-6",
    "react-particles-js": "^3.4.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "spotify-web-api-js": "^1.4.0",
    "styled-components": "^5.1.0",
    "use-image": "^1.0.6",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.shuffle": "^4.2.6",
    "@types/lodash.trim": "^4.5.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node": "^13.13.12",
    "@types/react": "^16.9.35",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/sinon": "^9.0.4",
    "@types/styled-components": "^5.1.0",
    "@types/superagent": "^4.1.7",
    "@types/webfontloader": "^1.6.32",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.2",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.2",
    "stylelint": "^13.5.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn lint && yarn build && yarn export # && yarn lint:fix"
    }
  }
}
