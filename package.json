{
  "name": "spotilities",
  "version": "0.7.8",
  "license": "MIT",
  "private": false,
  "browserslist": [
    "> 0.25%, not dead",
    "not op_mini all",
    "not ie > 0"
  ],
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "dev:static": "yarn build && yarn export && yarn start:static",
    "build": "next build",
    "export": "next export",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:static": "http-server ./out",
    "prod": "yarn start",
    "lint": "echo \" \n\nlinting all JS and TS files.\n\n\" && yarn lint:ts; echo \"\n\nLinting styled components.\n\n\" && yarn lint:css",
    "lint:fix": "echo \" \n\nlinting all JS and TS files.\n\n\" && yarn lint:ts:fix ; echo \"\n\nLinting styled components.\n\n\" && yarn lint:css",
    "lint:ts": "eslint ./**/*.{js,jsx,tsx,ts,css}",
    "lint:ts:fix": "eslint ./**/*.{js,jsx,tsx,ts,css} --fix",
    "lint:css": "stylelint ./**/*.{js,jsx,tsx,ts}",
    "tsc": "tsc .",
    "test": "jest",
    "test:init": "jest --init",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.55",
    "@next/bundle-analyzer": "^12.0.7",
    "@reduxjs/toolkit": "^1.3.4",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "canvas": "^2.8.0",
    "konva": "^8.3.1",
    "lodash.chunk": "^4.2.0",
    "lodash.shuffle": "^4.2.0",
    "lodash.trim": "^4.5.1",
    "lodash.uniq": "^4.5.0",
    "next": "^12.0.7",
    "next-pwa": "^5.4.4",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.2",
    "react-color": "^2.19.3",
    "react-color-extractor": "^1.1.2",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-konva": "17.0.2-5",
    "react-particles-js": "^3.4.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "spotify-web-api-js": "^1.4.0",
    "styled-components": "^5.1.0",
    "tsparticles": "^1.37.5",
    "use-image": "^1.0.6",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.1.1",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.shuffle": "^4.2.6",
    "@types/lodash.trim": "^4.5.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/sinon": "^10.0.6",
    "@types/styled-components": "^5.1.0",
    "@types/superagent": "^4.1.7",
    "@types/webfontloader": "^1.6.32",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "http-server": "^14.0.0",
    "jest": "^27.4.4",
    "jest-styled-components": "^7.0.2",
    "react-test-renderer": "^17.0.2",
    "sinon": "^12.0.1",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.3"
  }
