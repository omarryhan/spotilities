{
  "name": "spotilities",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "dev:static": "yarn build && yarn export && yarn start:static",
    "build": "next build",
    "export": "next export",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:static": "http-server ./out",
    "prod": "yarn start",
    "lint": "echo \" \n\nlinting all JS and TS files.\n\n\" && eslint ./**/*.{js,jsx,tsx,ts,css} && echo \"\n\nLinting styled components.\n\n\" && stylelint ./**/*.{js,jsx,tsx,ts}",
    "lint:fix": "echo \" \n\nlinting all JS and TS files.\n\n\" && eslint ./**/*.{js,jsx,tsx,ts,css} --fix; echo \"\n\nLinting styled components.\n\n\" && stylelint ./**/*.{js,jsx,tsx,ts} --fix",
    "tsc": "tsc .",
    "test": "jest",
    "test:init": "jest --init",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@next/bundle-analyzer": "^9.3.5",
    "@reduxjs/toolkit": "^1.3.4",
    "@sentry/browser": "^5.15.5",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "axios": "^0.19.2",
    "formik": "^2.1.4",
    "next": "9.3.5",
    "next-pwa": "^2.4.1",
    "next-redux-wrapper": "^6.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "^2.7.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.0",
    "styled-media-query": "^2.1.2",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/next": "^9.0.0",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/sinon": "^9.0.4",
    "@types/styled-components": "^5.1.0",
    "@types/superagent": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.2",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.2",
    "stylelint": "^13.5.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test # && yarn lint:fix"
    }
  }
}
