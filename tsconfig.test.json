{
    "compilerOptions": {
        "jsx": "react",
        "allowJs": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "target": "es5",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "isolatedModules": true
    }
  }
  